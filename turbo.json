{
	"$schema": "https://turborepo.com/schema.v2.json",
	"globalPassThroughEnv": ["GITHUB_ACTIONS", "CI"],
	"tasks": {
		"build:prod": {
			"cache": true,
			"dependsOn": ["types:check"],
			"inputs": [
				"$TURBO_DEFAULT$",
				"$TURBO_ROOT$/package.json",
				"$TURBO_ROOT$/pnpm-lock.yml",
				"$TURBO_ROOT$/pnpm-workspace.yml",
				"package.json",
				"rslib.config.ts",
				"tsconfig.json",
				"src/**/*.ts",
				"README.md",
				"LICENSE"
			],
			"outputLogs": "full",
			"outputs": ["dist/**"]
		},
		"types:check": {
			"cache": true,
			"dependsOn": [],
			"inputs": ["*.ts", "lib/**/*.ts", "lib/**/*.mts"],
			"outputLogs": "full",
			"outputs": []
		}
	}
}
