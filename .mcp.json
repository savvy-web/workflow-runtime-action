{
	"mcpServers": {
		"github": {
			"command": "docker",
			"args": ["run", "-i", "--rm", "-e", "GITHUB_PERSONAL_ACCESS_TOKEN", "-e", "GITHUB_HOST"],
			"image": "ghcr.io/github/github-mcp-server:latest"
		},
		"playwright": {
			"command": "npx",
			"args": ["-y", "@playwright/mcp@latest"],
			"env": {
				"PLAYWRIGHT_BROWSERS_PATH": "0"
			}
		}
	}
}
